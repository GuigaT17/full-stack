.js","original":{"line":46,"column":52}},{"generated":{"line":112,"column":1},"source":"components/Generation.js","original":{"line":46,"column":27}},{"generated":{"line":112,"column":2},"source":"components/Generation.js","original":{"line":46,"column":0}},{"name":"componentConnector","generated":{"line":114,"column":15},"source":"components/Generation.js","original":{"line":48,"column":15}},{"generated":{"line":114,"column":33},"source":"components/Generation.js","original":{"line":48,"column":33}},{"name":"Generation","generated":{"line":114,"column":34},"source":"components/Generation.js","original":{"line":48,"column":34}},{"generated":{"line":114,"column":44},"source":"components/Generation.js","original":{"line":48,"column":33}}],"sources":{"components/Generation.js":"import React, {Component} from 'react'\nimport {connect} from 'react-redux'\nimport {fetchGeneration} from '../actions/generation'\nimport fetchStates from '../reducer/fetchStates'\n\nconst MIN_DELAY = 3000\n\nclass Generation extends Component {\n    timer = null\n\n    componentDidMount(){\n        this.fetchNextGeneration()\n    }\n\n    componentWillUnmount(){\n        clearTimeout(this.timer)\n    }\n\n    fetchNextGeneration = () => {\n        this.props.fetchGeneration()\n        let delay = new Date(this.props.generation.expiration).getTime() - new Date().getTime()\n        if(delay < MIN_DELAY){\n            delay = MIN_DELAY\n        }\n        this.timer = setTimeout(() => this.fetchNextGeneration(), delay)\n    }\n\n    render() {\n        const {generation} = this.props\n        if(generation.status === fetchStates.error){\n            return <div>{generation.message}</div>\n        }\n        return(\n            <div>\n                <h3>Generation {generation.generationId}. Expires on:</h3>\n                <h4>{new Date(generation.expiration).toString()}</h4>\n            </div>\n        )\n    }\n}\nconst mapStateToProps = (state) => {\n    const generation = state.generation\n    return {generation}\n}\n\nconst componentConnector = connect(mapStateToProps, {fetchGeneration})\n\nexport default componentConnector(Generation)"},"lineCount":null}},"error":null,"hash":"c809e21d9afcc0d155fda03c73f252f4","cacheData":{"env":{}}}